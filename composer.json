{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-composer": "*",
        "yiisoft/yii2-imagine": "*",

        "codeless/ziparchiveex": "v1.0.1",
        "kartik-v/yii2-krajee-base": "*",
        "kartik-v/yii2-widget-activeform": "*",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-money": "*",
        "kartik-v/bootstrap-popover-x": "*",
        "kartik-v/yii2-popover-x": "*",
        "kartik-v/yii2-editable": "*",
        "kartik-v/yii2-checkbox-x": "*",
        "kartik-v/yii2-grid": "*",
        "kartik-v/yii2-widget-fileinput": "*",
        "kartik-v/yii2-detail-view": "*",
        "kartik-v/yii2-datecontrol": "@dev",
        "kartik-v/yii2-widget-select2": "@dev",
        "yiisoft/yii2-jui": "*",
        "bootflat/bootflat": "*",
        "bower-asset/spectrum": "*",
        "bower-asset/jquery-ui-touch-punch-improved": "*",
        "bower-asset/jquery-placeholder": "*",
        "bower-asset/bootstrap-switch": "*",
        "bower-asset/bootstrap-select": "*",
        "bower-asset/perfect-scrollbar": "*",
        "bower-asset/history.js": "*",
        "bower-asset/animate.css": "*",
        "bower-asset/nanoscroller": "*",
        "bower-asset/metisMenu": "*",
        "richthegeek/phpsass": "*",
        "nizsheanez/yii2-asset-converter": "1.*",
        "components/modernizr": "dev-master",
        "bower-asset/font-awesome": "dev-master",
        "bower-asset/js-cookie": "dev-master",
        "bower-asset/bootbox.js": "^4.4@dev",
        "bower-asset/underscore": "dev-master",
        "bower-asset/jQuery-Storage-API": "^1.7@dev",
        "bazilio/yii2-stubs-generator": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
